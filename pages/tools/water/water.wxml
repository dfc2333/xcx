<!--water.wxml-->
<view class="block">
  <t-navbar
    t-class-placeholder="t-navbar-placeholder"
    t-class-content="t-navbar-content"
    title="沸点算海拔"
    t-class-title="nav-title"
    left-arrow
    style="--td-navbar-color: #000;--td-navbar-bg-color:#dfc;"
  />
</view>
<scroll-view class="scrollarea" scroll-y>
  <t-input label="水沸点" placeholder="请输入测得的水沸点" value="{{empty ? null : boil}}" bindchange="calc"></t-input>
  <view class="container">
      <text class="text-center" style="width:700rpx; border: 1px solid grey;border-radius: 10rpx;">{{res}}</text>
  </view>
</scroll-view>
