<!--index.wxml-->
<view class="block">
  <t-navbar
    t-class-placeholder="t-navbar-placeholder"
    t-class-content="t-navbar-content"
    title="联系"
    t-class-title="nav-title"
    left-arrow
    style="--td-navbar-color: #000;--td-navbar-bg-color:#dfc;"
  />
</view>
<scroll-view class="scrollarea" scroll-y>
  <view class="container">
    <text style="position: relative; left: 1rpx; top: 325rpx">{{text}}</text>
    <button type="primary" bind:tap="back" style="position: relative; left: 0rpx; top: -304rpx">返回</button>
  </view>
</scroll-view>
