/**p1.wxss**/
.scrollarea {
  flex: 1;
  overflow-y: hidden;
}
.container {
  align-items: center;
  justify-content: start;
  padding: 0;
  box-sizing: border-box;
}
.text-center {
  margin-left: -50rpx;
  margin-right: -50rpx;
}
.ui {
  border: 1px solid gray;
  border-radius: 10rpx;
  width: 700rpx;
}
.input-example {
  --td-input-vertical-padding: 24rpx;
  background-color: var(--bg-color-demo);
  padding: 32rpx 32rpx 16rpx;
}

.input-example__label {
  color: var(--td-text-color-primary);
  font-size: 24rpx;
  line-height: 40rpx;
  margin: 0 8rpx 16rpx;
}
page {
  height: 100%;
  width: 750rpx;
  display: flex;
  flex-direction: column;
  background: linear-gradient(#eeeeeeee, #eeeeeeee);
}
.img {
  width: 100%;
  height: 100%;
}
.block {
  display: block;
  margin-bottom: 32rpx;
}
